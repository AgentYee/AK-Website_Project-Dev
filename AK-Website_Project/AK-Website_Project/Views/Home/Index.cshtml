@model AK_Website_Project.Models.ViewModels.Home.HomeViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid">
    <div class="page-header text-center">
        <h1>
            <label>Aura Kingdom - Refining Resources</label>
        </h1>
    </div>
    <div class="form-group">
        <div>
            <div class="col-xs-2 dropdown">
                <button class="btn btn-info dropdown-toggle" type="button" data-toggle="dropdown">
                    Crafting
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    @foreach (var item in Model.CraftingList)
                    {
                        <li><a href="#">@item</a></li>
                    }
                </ul>
            </div>

            <div class="col-xs-2 dropdown">
                <button class="btn btn-info dropdown-toggle" type="button" data-toggle="dropdown">
                    Fishing
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    @foreach (var item in Model.FishingList)
                    {
                        <li><a href="#">@item</a></li>
                    }
                </ul>
            </div>

            <div class="col-xs-2 dropdown">
                <button class="btn btn-info dropdown-toggle" type="button" data-toggle="dropdown">
                    Gathering
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    @foreach (var item in Model.GatheringList)
                    {
                        <li><a href="#">@item</a></li>
                    }
                </ul>
            </div>

            <div class="col-xs-2 dropdown">
                <button class="btn btn-info dropdown-toggle" type="button" data-toggle="dropdown">
                    Alchemy
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    @foreach (var item in Model.AlchemyList)
                    {
                        <li><a href="#">@item</a></li>
                    }
                </ul>
            </div>

            <div class="col-xs-2 dropdown">
                <button class="btn btn-info dropdown-toggle" type="button" data-toggle="dropdown">
                    Cooking
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    @foreach (var item in Model.CookingList)
                    {
                        <li><a href="#">@item</a></li>
                    }
                </ul>
            </div>
            <div class="col-xs-2">
                TestsetsetTestsetsetTestsetsetTestsetsetTest
            </div>
        </div>
    </div>
</div>

